Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MyCentralMoment", "[", 
    RowBox[{"l_List", ",", "r_List"}], "]"}], ":=", 
   FractionBox[
    RowBox[{"Plus", "@@", 
     RowBox[{"Times", "@@@", 
